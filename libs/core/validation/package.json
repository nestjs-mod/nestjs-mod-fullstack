{
    "name": "@nestjs-mod-fullstack/validation",
    "version": "0.0.1",
    "dependencies": {
        "tslib": "^2.3.0"
    },
    "type": "commonjs",
    "main": "./src/index.js",
    "typings": "./src/index.d.ts",
    "i18n": [
        {
            "scope": "validation",
            "path": "src/i18n",
            "strategy": "join"
        },
        {
            "scope": "validation-getText",
            "path": "src/i18n/getText",
            "strategy": "join"
        }
    ]
}