{
    "version": 2,
    "builds": [
        {
          "src": "package.json",
          "use": "@vercel/static-build",
          "config": {
            "distDir": "out"
          }
        },
        {
            "src": "/out/apps/server/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/out/apps/server/main.js"
        },
        { "src": "/assets/(.*)", "dest": "/assets/$1" },
        {
            "src": "/(.*)",
            "dest": "/out/apps/client/browser/index.html"
        }
    ]
}