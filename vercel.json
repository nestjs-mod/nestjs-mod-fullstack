{
    "version": 2,
    "builds": [
        {
            "src": "apps/client/browser/**",
            "use": "@vercel/static"
        },
        {
            "src": "apps/server/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "apps/server/main.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "apps/client/browser/assets/$1"
        },
        {
            "src": "/(.*)\\.(.*)",
            "dest": "apps/client/browser/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "apps/client/browser/index.html"
        }
    ]
}