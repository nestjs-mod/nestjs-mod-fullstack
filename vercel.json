{
    "version": 2,
    "builds": [
        {
            "src": "out/apps/server/main.js",
            "use": "@vercel/node"
        },
        {
            "src": "out/apps/client/browser/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "out/apps/server/main.js"
        }
    ]
}