{
    "version": 2,
    "builds": [
        {
            "src": "/out/apps/server/main.js",
            "use": "@vercel/node"
        },
        {
          "src": "ls out/apps/client/browser",
          "use": "@vercel/static-build",
          "config": {
            "distDir": "/out/apps/client/browser"
          }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/out/apps/server/main.js"
        },
        {
            "src": "/",
            "dest": "/out/apps/client/browser/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "/out/apps/client/browser/$1"
        }
    ]
}